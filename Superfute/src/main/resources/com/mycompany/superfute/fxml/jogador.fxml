<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="635.0" prefWidth="455.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.superfute.JogadorController">
   <children>
      <Label fx:id="labelNomeEquipa" alignment="CENTER" layoutX="145.0" layoutY="29.0" text="JOGADORES">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <Button fx:id="btnVoltar" layoutX="350.0" layoutY="587.0" mnemonicParsing="false" onAction="#btnVoltar" prefHeight="34.0" prefWidth="80.0" text="Voltar" />
      <Button fx:id="btnVerJogador" layoutX="165.0" layoutY="503.0" mnemonicParsing="false" onAction="#btnVerJogador" prefHeight="51.0" prefWidth="126.0" text="VER JOGADOR" />
      <ScrollPane layoutX="25.0" layoutY="80.0" prefHeight="410.0" prefWidth="405.0">
         <content>
            <TableView fx:id="listaJogadores" prefHeight="408.0" prefWidth="403.0">
               <columns>
                  <TableColumn fx:id="colunaJogador" prefWidth="134.0" text="Jogador" />
                  <TableColumn fx:id="colunaEquipa" prefWidth="134.0" text="Equipa" />
               </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </content>
      </ScrollPane>
   </children>
</AnchorPane>
