<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="543.0" prefWidth="352.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.superfute.PessoaController">
   <children>
      <Label fx:id="labelNomeEquipa" alignment="CENTER" layoutX="130.0" layoutY="14.0" text="PESSOAS">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <Button fx:id="btnInserirPessoa" layoutX="26.0" layoutY="443.0" mnemonicParsing="false" onAction="#btnInserirPessoa" prefHeight="34.0" prefWidth="93.0" text="Inserir " />
      <Button fx:id="btnEditarPessoa" layoutX="132.0" layoutY="443.0" mnemonicParsing="false" onAction="#btnEditarPessoa" prefHeight="34.0" prefWidth="93.0" text="Editar " />
      <Button fx:id="btnVoltar" layoutX="258.0" layoutY="494.0" mnemonicParsing="false" onAction="#btnVoltar" prefHeight="34.0" prefWidth="80.0" text="Voltar" />
      <ScrollPane layoutX="23.0" layoutY="62.0" prefHeight="374.0" prefWidth="302.0">
         <content>
            <TableView fx:id="listaPessoas" prefHeight="372.0" prefWidth="300.0">
               <columns>
                  <TableColumn fx:id="colunaNome" prefWidth="134.0" text="Nome" />
                  <TableColumn fx:id="colunaNacionalidade" prefWidth="134.0" text="Nacionalidade" />
               </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </content>
      </ScrollPane>
      <Button fx:id="btnRemoverPessoa" layoutX="233.0" layoutY="443.0" mnemonicParsing="false" onAction="#btnRemoverPessoa" prefHeight="34.0" prefWidth="93.0" text="Remover " />
   </children>
</AnchorPane>
